{
  "singleQuote": true,
  "trailingComma": "all",
  "useTabs": true,
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "semi": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^@providers/(.*)$",
    "^@services/(.*)$",
    "^@assets/(.*)$",
    "^@types/(.*)$",
    "^@hooks/(.*)$",
    "^@store/(.*)$",
    "^@utils/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "^(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
